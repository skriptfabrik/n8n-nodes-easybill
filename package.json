{
	"name": "n8n-nodes-easybill",
	"version": "1.0.3",
	"description": "Integration der EasyBill Api in n8n",
	"license": "MIT",
	"homepage": "https://paulkolle.de",
	"keywords": [
		"n8n-community-node-package",
		"easybill"
	],
	"author": {
		"name": "Paul",
		"email": "pk@paulkolle.de"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/paulkolle/n8n-easybill-node"
	},
	"scripts": {
		"build": "n8n-node build",
		"build:watch": "tsc --watch",
		"dev": "n8n-node dev",
		"lint": "n8n-node lint",
		"lint:fix": "n8n-node lint --fix",
		"release": "n8n-node release",
		"prepublishOnly": "n8n-node prerelease",
		"format": "prettier --write .",
		"format:check": "prettier --check ."
	},
	"files": [
		"dist"
	],
	"n8n": {
		"n8nNodesApiVersion": 1,
		"strict": true,
		"credentials": [
			"dist/credentials/EasyBillApi.credentials.js"
		],
		"nodes": [
			"dist/nodes/EasyBill/EasyBill.node.js"
		]
	},
	"devDependencies": {
		"@n8n/node-cli": "*",
		"eslint": "9.32.0",
		"prettier": "3.6.2",
		"release-it": "^19.0.4",
		"typescript": "5.9.2"
	},
	"peerDependencies": {
		"n8n-workflow": "*"
	}
}
